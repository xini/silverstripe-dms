{
    "name": "silverstripe/dms",
    "description": "Adds a Document Management System to SilverStripe ",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "dms", "document", "document management system"],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Julian Seidenberg",
        "email": "julian@silverstripe.com"
    }],
    "require": {
        "silverstripe/framework": "~4.0",
        "silverstripe/cms": "~4.0",
        "symbiote/silverstripe-gridfieldextensions": "3.2.2",
        "silverstripe/taxonomy": "2.0.2",
        "mustangostang/spyc": "0.6.3"
    },
    "suggest": {
        "undefinedoffset/sortablegridfield": "Allow documents to be reordered via drag-and-drop"
    },
    "autoload": {
        "psr-4": {
            "SilverStripeDMS\\": "code/"
        }
    },
    "extra": {
        "expose": [
            "dist",
            "javascript",
            "images"
        ]
    }
}
